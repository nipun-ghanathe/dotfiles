[user]
	name = nipun
	email = nipuncod@gmail.com
[init]
	defaultBranch = main
[credential]
	helper = store
[core]
	autocrlf = input
	safecrlf = true
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
