set modeindicatorshowkeys true
set editorcmd foot -e nvim "%f"
set viewsource default
set subconfigs null

set searchurls.youtube https://www.youtube.com/results?search_query=
set searchurls.yt youtube
set searchurls.gh https://www.github.com/

bind J tabnext
bind K tabprev

bind yp composite js document.location.pathname.replace(/^\/+/, "") | clipboard yank
bindurl https://github.com yg composite js "git clone " + document.location.origin + document.location.pathname.replace(/^((\/[^/]+){2}).*$/, "$1") | clipboard yank
